{
  "name": "tree-sitter-bemcss",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for BEM CSS in Rust",
  "main": "bindings/node",
  "types": "bindings/node/index.d.ts",
  "scripts": {
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "dependencies": {
    "node-addon-api": "^7.0.0",
    "node-gyp-build": "^4.0.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8",
    "prebuildify": "^6.0.0",
    "tree-sitter-rust": "^0.21.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.20.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "keywords": [
    "tree-sitter",
    "rust",
    "bem",
    "css",
    "parser",
    "syntax"
  ],
  "author": "Assistant",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/user/tree-sitter-bemcss"
  },
  "tree-sitter": [
    {
      "scope": "source.rust.bemcss",
      "path": "bemcss"
    },
    {
      "scope": "source.rust.rust_with_bemcss",
      "path": "rust_with_bemcss",
      "file-types": ["rs"],
      "content-regex": "#\\[bem\\(\""
    }
  ]
}
